{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "axe/structure": "off",
    "disown-opener": "off",
    "meta-viewport": "off",
    "meta-charset-utf-8": "off",
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off",
        "frame-title": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[referrerpolicy=no-referrer-when-downgrade]"
        ]
      }
    ]
  }
}